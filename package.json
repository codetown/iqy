{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/preset-react": "2.1.4",
    "@umijs/test": "^3.5.26",
    "antd": "^4.21.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "umi": "^3.5.26"
  },
  "devDependencies": {
    "lint-staged": "^13.0.2",
    "prettier": "^2.2.0",
    "yorkie": "^2.0.0"
  }
}
